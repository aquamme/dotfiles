set nocompatible
set t_Co=256
set cursorcolumn 
set cursorline
colorscheme molokai
syntax on

hi StatusLine term=bold,reverse cterm=bold ctermbg=101 ctermfg=16 gui=bold guibg=#808070 guifg=#000000
